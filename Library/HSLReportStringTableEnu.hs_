	Insert(IDS::traceCreateReportFile,						"HSLReportLib - CreateReportFile"				);
	Insert(IDS::traceCreateATBarcodeFile,					"HSLReportLib - CreateATBarcodeFile"			);
	Insert(IDS::trackerOFF,										" > Sample tracking is OFF"						);
	Insert(IDS::traceElement,									" > Successful: labware '%s'"						);
	Insert(IDS::emptyReportList,								" > No labware found in the report list"		);
}
