// }} ""
// {{ 5 "wait_1000_milliseconds" "InitLocals"
// }} ""

{variable tempTimer;
    timer timerOne;

         tempTimer = 1.0; //second to wait

                  //wait 1.0 seconds 
                     timerOne.SetTimer(tempTimer);
