// {{{ 5 "DeleteFile" "Begin"
private function DeleteFile( variable f ) void {
// }} ""
// {{ 5 "DeleteFile" "InitLocals"
// }} ""
{ 
         object objFileSystemObject;
         variable blnFileExists;

         //file exists?
         objFileSystemObject.CreateObject("Scripting.FileSystemObject");
         blnFileExists = objFileSystemObject.FileExists(f);
         objFileSystemObject.ReleaseObject();

         if(blnFileExists != hslFalse)
         {
            //delete file
            objFileSystemObject.CreateObject("Scripting.FileSystemObject");
            objFileSystemObject.DeleteFile(f);
            objFileSystemObject.ReleaseObject();
         }
         else
         {
            //Trace("Cant delete file, not found " , f);
         }
}
// {{{ 5 "DeleteFile" "End"
}
