// {{{ 5 "Convert_Seconds_to_HourMinSec" "Begin"
private function Convert_Seconds_to_HourMinSec( variable iSeconds, variable & o_hours, variable & o_min, variable & o_sec ) void {
// }} ""
// {{ 5 "Convert_Seconds_to_HourMinSec" "InitLocals"
o_hours = 0;
o_min = 0;
o_sec = 0;
// }} ""

  o_min = MthFloor (iSeconds / 60) ;
  o_hours = MthFloor(o_min / 60);
  o_sec = iSeconds - 60*o_min;
  o_min = o_min - o_hours * 60;
// {{{ 5 "Convert_Seconds_to_HourMinSec" "End"
}
