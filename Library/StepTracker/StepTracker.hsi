// {{{ 5 "GetNowTime" "Begin"
private function GetNowTime(  ) variable {
// }} ""
private variable strScriptResult;
// {{ 5 "GetNowTime" "InitLocals"
// }} ""
// {{ 137 1 0 "0349d4f1_db59_454c_a95fd40dd33faa64" "{B31F3543-5D80-11d4-A5EB-0050DA737D89}"
strScriptResult = 0.0;
// }} ""
{
         object objScriptControl;		
         object objFileSystemObject;

         //get directories
         objFileSystemObject.CreateObject("Scripting.FileSystemObject");
         objScriptControl.CreateObject("MSScriptControl.ScriptControl");

         // - create script
			objScriptControl.Language = "VBScript";
			objScriptControl.AddObject("fso", objFileSystemObject);	
         
        			objScriptControl.AddCode
			("																	\n\
				Option Explicit											\n\
																				\n\
				Function GetTimeNow()        						   \n\
               Dim t                                        \n\
               Dim ms                                       \n\
               t = Timer()												\n\
               ms = Int(t * 100000)			                  \n\
               GetTimeNow = 	CStr(ms)     					   \n\
				End Function												\n\
			");

         // - execute script      
		   strScriptResult = objScriptControl.Eval("GetTimeNow()");
          
         //Trace(strScriptResult) ;       

         // - clean up
         objScriptControl.ReleaseObject();
         objFileSystemObject.ReleaseObject();
 
        		
}
// {{{ 139 1 0 "5b4ebf23_44e5_4b36_b7501605c3fc8dfa" "{9EC997CD-FD3B-4280-811B-49E99DCF062C}"
return (strScriptResult);
// }} ""
// {{{ 5 "GetNowTime" "End"
}
